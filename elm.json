{
    "type": "package",
    "name": "sparksp/elm-review-ports",
    "summary": "Provides elm-review rules to detect problematic elm ports.",
    "license": "MIT",
    "version": "1.3.0",
    "exposed-modules": [
        "NoDuplicatePorts",
        "NoUnsafePorts",
        "NoUnusedPorts"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.5 <= v < 2.0.0",
        "jfmengels/elm-review": "2.6.0 <= v < 3.0.0",
        "stil4m/elm-syntax": "7.2.8 <= v < 8.0.0"
    },
    "test-dependencies": {
        "elm/project-metadata-utils": "1.0.2 <= v < 2.0.0",
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}
